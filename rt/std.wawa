func memcpy1rl
dst i32 src i32 len i32 void
	loop
		load len
		i32.eqz
		br_if 1

		load dst
		load len
		i32.add
		load src
		load len
		i32.add
		i32.load8_u 
		i32.store8

		load len
		i32 1
		i32.sub
		store len
	end

func memcpy8
dst i32 src i32 len i32 void
n i32
	loop
		load n
		load len
		i32.eq
		br_if 1

		load dst
		load n
		i32.add
		load src
		load n
		i32.add
		i64.load
		i64.store

		load n
		i32 8
		i32.add
		store n
	end
